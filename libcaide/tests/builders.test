#!/bin/bash

"$CAIDE" problem prob
cat <<EOF >prob/case1.in
1
2
3
EOF
cp prob/case1.in prob/case1.out

"$CAIDE" lang cpp


cat <<EOF >prob/prob.cpp
#include <istream>
#include <ostream>
using namespace std;
void solve(istream& in, ostream& out) {
    int n;
    while (in >> n)
        out << n << endl;
}
EOF

"$CAIDE" test
sed -i 's/#time:[^ ]* //' prob/.caideproblem/test/report.txt
compare_with after-test prob/.caideproblem/test/report.txt

"$CAIDE" lang csharp

cat <<EOF >prob/prob.cs
using System;
using System.IO;
class Solution {
    public void solve(TextReader input, TextWriter output) {
        string line;
        while ((line = input.ReadLine()) != null) {
            output.WriteLine(line);
        }
    }
}
EOF

rm prob/.caideproblem/test/{report.txt,*.in,*.out}

"$CAIDE" test
sed -i 's/#time:[^ ]* //' prob/.caideproblem/test/report.txt
compare_with after-test prob/.caideproblem/test/report.txt

